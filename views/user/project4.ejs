<script>
    const prevButton = document.querySelector('.custom-prev');
const nextButton = document.querySelector('.custom-next');
const carouselInner = document.querySelector('.custom-carousel-inner');
const indicators = document.querySelectorAll('.custom-indicator');

let index = 0;

function showSlide(i) {
    if (i >= indicators.length) i = 0;
    if (i < 0) i = indicators.length - 1;

    carouselInner.style.transform = `translateX(-${i * 100}%)`;

    indicators.forEach((indicator, idx) => {
        indicator.classList.toggle('active', idx === i);
    });

    index = i;
}

prevButton.addEventListener('click', () => {
    showSlide(index - 1);
});

nextButton.addEventListener('click', () => {
    showSlide(index + 1);
});

indicators.forEach((indicator, idx) => {
    indicator.addEventListener('click', () => {
        showSlide(idx);
    });
});

// Optionally, you can start the carousel with automatic sliding
setInterval(() => {
    showSlide(index + 1);
}, 5000); // Change slide every 5 seconds

</script>
